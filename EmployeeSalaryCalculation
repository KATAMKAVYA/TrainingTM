class EmployeeSalaryCalculation {
    public double calculateSalary() {
        return 50000;
    }
}

class Manager extends EmployeeSalaryCalculation {
    @Override
    public double calculateSalary() {
        return super.calculateSalary() + 10000;
    }

    public static void main(String[] args) {
        EmployeeSalaryCalculation emp = new EmployeeSalaryCalculation();
        Manager mgr = new Manager();
        System.out.println("Base salary: " + emp.calculateSalary());
        System.out.println("Base salary + Bonus: " + mgr.calculateSalary());
    }
}
